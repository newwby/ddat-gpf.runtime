[application]

config/name="#[ddat-gpf.runtime] Runtime Scaffold"
config/description="workspace to focus on developing a series of runtime scenes for quick prototyping"
run/main_scene="res://src/runtime/main.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="ddat_framework"
config/icon="res://art/splash/ddat_icon.png"

[autoload]

GlobalData="*res://src/autoload/global_data.gd"
GlobalDebug="*res://src/autoload/global_debug.gd"
GlobalDevTools="*res://src/autoload/global_devtools.tscn"
GlobalInput="*res://src/input_manager/autoload/global_input.gd"
GlobalConfig="*res://src/config_manager/autoload/global_config.gd"
GlobalProgression="*res://src/file_manager/autoload/global_progression.gd"

[display]

window/size/width=1920
window/size/height=1080

[editor]

script_templates_search_path="res://_dev/default_scripts"

[logging]

file_logging/enable_file_logging=true

[network]

limits/debugger_stdout/max_chars_per_second=8192
